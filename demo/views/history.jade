extends ./base.jade

block styles
  //- link(href="/css/new.css" rel="stylesheet" type="text/css")

block content
  h1= History

  ul.list-group

    each val in history
      li.list-group-item.test
        .message= val.message
        .commit= val.commit
        .author= val.author
        .date= val.date
        a(href="http://localhost:8080/document/diff/" + id + "/" + val.commit).btn.btn-secondary#btn-diff Diff
  
  block scripts
    script(src="/scripts/history.js")
